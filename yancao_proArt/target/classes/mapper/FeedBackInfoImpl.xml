<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.aistrong.hnyc_codecenter.task.dao.FeedBackInfoDao">
    <resultMap id="ResultMap" type="com.aistrong.hnyc_codecenter.task.entity.FeedBackInfo">
        <id column="MSG_ID" property="msg_id" jdbcType="VARCHAR"/>
        <result column="STATE_CODE" property="state_code" jdbcType="VARCHAR"/>
        <result column="STATE_DESC" property="state_desc" jdbcType="VARCHAR"/>
        <result column="WS_MARK" property="ws_mark" jdbcType="VARCHAR"/>
        <result column="WS_METHOD" property="ws_method" jdbcType="VARCHAR"/>
        <result column="WS_PARAM" property="ws_param" jdbcType="VARCHAR"/>
        <result column="CURR_TIME" property="curr_time" jdbcType="TIMESTAMP"/>
        <result column="SEND_CONTENT" property="send_content" jdbcType="VARCHAR"/>
    </resultMap>

    <insert id="addFeedBackInfo" parameterType="com.aistrong.hnyc_codecenter.task.entity.FeedBackInfo">
        INSERT INTO HNYC_FEEDBACK_INFO (msg_id, state_code, state_desc, ws_mark, ws_method, ws_param, curr_time, send_content)
        VALUES (
        #{msg_id, jdbcType=VARCHAR},
        #{state_code, jdbcType=VARCHAR},
        #{state_desc, jdbcType=VARCHAR},
        #{ws_mark, jdbcType=VARCHAR},
        #{ws_method, jdbcType=VARCHAR},
        #{ws_param, jdbcType=VARCHAR},
        #{curr_time, jdbcType=TIMESTAMP},
        #{send_content, jdbcType=VARCHAR}
        )
    </insert>
</mapper>